.box{
  width: 100%;
  background-color: #EDEEF0;
  padding-top:30rpx;
  height:1190rpx;
}
.posters-box{
  width: 85%;
  margin: 0 auto;
  border-radius: 14rpx;
  background-color: #fff;
  padding: 20rpx;
  box-shadow: #E4E6F2 0 0 10rpx 5rpx;
  margin-top:30rpx;
}
.nav-all{
  overflow: hidden;
}
.nav-img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 14rpx;
  float: left;
  margin-right: 40rpx;
}
.nav-img image{
  border-radius:14rpx;
  width: 100%;
  height: 100%;
}
.nav-text{
overflow:hidden;
}
.nav-text text{
  float: left;
  display: block;
  width: 100%;
}
.nav-name{
  font-size: 20px;
  margin-bottom: 20rpx;
}
.nav-work{
  font-size: 18px;
}
.nav-card{
  color: #aaa;
}
.border{
  border-bottom: 1px dashed #aaa;
  margin-top:50rpx;
  margin-bottom:30rpx;
}
.phone-box view{
  float: left;
}
.phone-box{
  overflow: hidden;
  margin-left:10rpx;
  margin-bottom:20rpx;
}
.phone{
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
  margin-top:7rpx;
}
.phone image{
  width: 100%;
  height: 100%;
}
.text-one{
  color: #B4B4B4;
  margin-right: 30rpx;
}
.text-two{
  color: #747474;
  font-size:12px;
}
.posters-icon{
  width:270rpx;
  height:270rpx;
  margin-top:60rpx;
  margin-left:200rpx;
}
.posters-icon image{
  width: 100%;
  height: 100%;
  }
.keep{
  width: 90%;
  height: 80rpx;
  line-height: 80rpx;
  margin: 100rpx auto 50rpx;
  border-radius: 14rpx;
  background-color: #4EA4FB;
}
.keep text{
  display: block;
  color: #FFF;
  text-align: center;
}
